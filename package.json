  "files": [
    "dist"
  ],
  "dependencies": {
    "@actions/github": "^1.1.0",
    "@types/jest": "^24.0.23",
    "@types/node": "^12.12.12",
    "@typescript-eslint/eslint-plugin": "^2.8.0",
    "@typescript-eslint/parser": "^2.8.0",
    "eslint": "^6.7.0",
    "jest": "^24.9.0",
    "jest-circus": "^24.9.0",
